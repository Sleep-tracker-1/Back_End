{
  "name": "sleep-tracker-api",
  "version": "1.0.0",
  "description": "API for the sleep tracker app",
  "repository": "https://github.com/Sleep-tracker-1/Back_End.git",
  "license": "MIT",
  "author": "Westley Feller <feller.westley@gmail.com>",
  "main": "./build/index.js",
  "typings": "./build/index",
  "scripts": {
    "build": "tsc",
    "check-format": "yarn run prettier --ignore-path .gitignore --list-different",
    "check-types": "tsc --watch",
    "format": "yarn run prettier --ignore-path .gitignore --write",
    "postinstall": "yarn run build",
    "lint": "eslint --ignore-path .gitignore --ext .ts .",
    "migrate:latest": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:rollback:testing": "cross-env NODE_ENV=testing npx knex migrate:rollback",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(json|js|ts)\"",
    "seed:production:bedhours": "cross-env NODE_ENV=production npx knex seed:run --specific=03-bedhours.js",
    "seed:production:user": "cross-env NODE_ENV=production npx knex seed:run --specific=02-user.js",
    "seed:run": "npx knex seed:run",
    "server": "tsc && nodemon ./build/index.js",
    "start": "node ./build/index.js",
    "test": "cross-env NODE_ENV=testing npx knex migrate:latest && cross-env NODE_ENV=testing npx knex seed:run && jest --watch --runInBand --detectOpenHandles",
    "validate": "npm-run-all --parallel check-format lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|ts)": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "bcrypt": "^4.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "folktale": "^2.3.2",
    "helmet": "^3.21.3",
    "jest-in-case": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.10",
    "knex-cleaner": "^1.3.0",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "swagger-ui-express": "^4.1.3",
    "ts-node": "^8.6.2",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.10",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.1.3",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.6",
    "@types/swagger-ui-express": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "sort-package-json": "^1.40.0"
  },
  "engines": {
    "node": "13.3.0",
    "yarn": "1.22.0"
  }
}
